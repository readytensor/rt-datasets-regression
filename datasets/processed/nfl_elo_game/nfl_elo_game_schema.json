        "NYJ",
        "NYY",
        "OAK",
        "PHI",
        "PIT",
        "SEA",
        "SF",
        "TB",
        "TEN",
        "WSH"
      ],
      "nullable": false
    },
    {
      "name": "team2",
      "description": "Team 2 involved in the game",
      "dataType": "CATEGORICAL",
      "categories": [
        "ARI",
        "ATL",
        "BAL",
        "BUF",
        "CAR",
        "CHI",
        "CIN",
        "CLE",
        "DAL",
        "DEN",
