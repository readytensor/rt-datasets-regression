        "TEN",
        "WSH"
      ],
      "nullable": false
    },
    {
      "name": "elo1",
      "description": "Elo rating of team 1 before game",
      "dataType": "NUMERIC",
      "example": 1598.581,
      "nullable": false
    },
    {
      "name": "elo2",
      "description": "Elo rating of team 2 before game",
      "dataType": "NUMERIC",
      "example": 1454.772,
      "nullable": false
    },
    {
      "name": "score1",
      "description": "Points scored by team 1 in the game",
