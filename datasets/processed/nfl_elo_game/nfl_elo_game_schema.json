{
  "title": "NFL Elo Game Dataset",
  "description": "This dataset contains ELO ratings for teams involved in each NFL game from 1951 through 2020 seasons. The task is to predict how many points team 2 scored, given various features for the game, including points scored by team 1.\n",
  "modelCategory": "regression",
  "schemaVersion": 1.0,
  "inputDataFormat": {
    "type": "CSV",
    "encoding": "utf-8"
  },
  "id": {
    "name": "game_id",
    "description": "Unique identifier for the game"
  },
  "target": {
    "name": "score2",
    "description": "Target - Points scored by team 2 in the game",
    "example": 14
  },
  "features": [
    {
      "name": "season",
      "description": "season year",
      "dataType": "NUMERIC",
      "example": 1951,
      "nullable": false
    },
    {
      "name": "month",
      "description": "month of the year",
      "dataType": "CATEGORICAL",
      "categories": [
        1,
        10,
        11,
        12,
        2,
        8,
        9
      ],
      "nullable": false
    },
    {
      "name": "neutral",
      "description": "Whether game venue was neutral",
      "dataType": "CATEGORICAL",
      "categories": [
        0,
        1
      ],
      "nullable": false
    },
    {
      "name": "playoff",
      "description": "Whether this was a playoff game",
      "dataType": "CATEGORICAL",
      "categories": [
        0,
        1
      ],
      "nullable": false
    },
    {
      "name": "team1",
      "description": "Team 1 involved in the game - has homefield unless venue is indicated as neutral",
      "dataType": "CATEGORICAL",
      "categories": [
        "ARI",
        "ATL",
        "BAL",
        "BUF",
        "CAR",
        "CHI",
        "CIN",
        "CLE",
        "DAL",
        "DEN",
        "DET",
